<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".LogoutActivity">

    <Button
            android:text="Logout"
            android:layout_width="88dp"
            android:layout_height="48dp"
            android:id="@+id/logout_btn" android:layout_marginEnd="162dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="500dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="162dp"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="183dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <Button
            android:text="Open Feed"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:id="@+id/open_feed_btn"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="183dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="161dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="162dp"
            app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginTop="270dp" app:layout_constraintTop_toBottomOf="@+id/logout_btn"/>

</android.support.constraint.ConstraintLayout>